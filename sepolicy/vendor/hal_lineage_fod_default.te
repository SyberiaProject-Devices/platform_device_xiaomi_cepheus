# Allow rw access to fod sysfs
type sysfs_fod, sysfs_type, fs_type;
#type sysfs_graphics, sysfs_type, fs_type;
allow vendor_fod sysfs_fod:file rw_file_perms;

# Allow vendor_fod to access graphics sysfs
r_dir_file(vendor_fod, vendor_sysfs_graphics)
allow vendor_fod vendor_sysfs_graphics:file rw_file_perms;

# Allow vendor_fod to hal_xiaomi_fingerprint_hwservice
allow vendor_fod hal_xiaomi_fingerprint_hwservice:hwservice_manager find;

# Allow binder communication with hal_fingerprint_default
binder_call(vendor_fod, hal_fingerprint_default)
